{
    "compilerOptions" : {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "build"
    },
    "include": [
        "core/**/*",
        "lib/**/*",
        "api/**/*",
        "trys/**/*",
        "app/*",
        "./*"
    ],
    "paths": {
        "@lib/*" : ["./build/lib/*"]
    }
}